affine seidel-2d {tsteps,n| tsteps-2>=0 && n-2>=0 }
	input
		double A {i0,i1| -i1+n-1>=0 && -i0+n-1>=0 && i1>=0 && i0>=0 && n-3>=0 } ;
	local
		double S0 {t,i,j| -t+tsteps-1>=0 && t>=0 && j-1>=0 && i-1>=0 && -i+n-2>=0 && -j+n-2>=0 } ;
	let
		S0[t,i,j] = case
			{ | t-1>=0 && -i+n-3>=0 && j-2>=0 && i-2>=0 && -j+n-3>=0 } : (S0[t,i-1,j-1] + S0[t,i-1,j] + S0[t,i-1,j+1] +
			S0[t,i,j-1] + S0[t-1,i,j] + S0[t-1,i,j+1] + S0[t-1,i+1,j-1] + S0[t-1,i+1,j] + S0[t-1,i+1,j+1]) / 9.0 ;
			{ | i-n+2==0 && j-2>=0 && t-1>=0 && -j+n-3>=0 } : (S0[t,i-1,j-1] + S0[t,i-1,j] + S0[t,i-1,j+1] + S0[t,i,j-1] +
			S0[t-1,i,j] + S0[t-1,i,j+1] + A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-n+2==0 && i-2>=0 && -i+n-3>=0 && t-1>=0 } : (S0[t,i-1,j-1] + S0[t,i-1,j] + A[i-1,j+1] + S0[t,i,j-1] +
			S0[t-1,i,j] + A[i,j+1] + S0[t-1,i+1,j-1] + S0[t-1,i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-n+2==0 && i-n+2==0 && t-1>=0 && n-4>=0 } : (S0[t,i-1,j-1] + S0[t,i-1,j] + A[i-1,j+1] + S0[t,i,j-1] +
			S0[t-1,i,j] + A[i,j+1] + A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | t==0 && i-2>=0 && -j+n-3>=0 && j-2>=0 } : (S0[0,i-1,j-1] + S0[0,i-1,j] + S0[0,i-1,j+1] + S0[0,i,j-1] + A[i,j] +
			A[i,j+1] + A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-n+2==0 && t==0 && i-2>=0 } : (S0[0,i-1,j-1] + S0[0,i-1,j] + A[i-1,j+1] + S0[0,i,j-1] + A[i,j] + A[i,j+1] +
			A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-1==0 && i-2>=0 && -i+n-3>=0 && t-1>=0 } : (A[i-1,j-1] + S0[t,i-1,j] + S0[t,i-1,j+1] + A[i,j-1] + S0[t-1,i,j] +
			S0[t-1,i,j+1] + A[i+1,j-1] + S0[t-1,i+1,j] + S0[t-1,i+1,j+1]) / 9.0 ;
			{ | j-1==0 && i-n+2==0 && t-1>=0 && n-4>=0 } : (A[i-1,j-1] + S0[t,i-1,j] + S0[t,i-1,j+1] + A[i,j-1] + S0[t-1,i,j] +
			S0[t-1,i,j+1] + A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-1==0 && t==0 && i-2>=0 } : (A[i-1,j-1] + S0[0,i-1,j] + S0[0,i-1,j+1] + A[i,j-1] + A[i,j] + A[i,j+1] +
			A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | i-1==0 && j-2>=0 && t-1>=0 && -j+n-3>=0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + S0[t,i,j-1] + S0[t-1,i,j] +
			S0[t-1,i,j+1] + S0[t-1,i+1,j-1] + S0[t-1,i+1,j] + S0[t-1,i+1,j+1]) / 9.0 ;
			{ | j-n+2==0 && i-1==0 && t-1>=0 && n-4>=0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + S0[t,i,j-1] + S0[t-1,i,j] +
			A[i,j+1] + S0[t-1,i+1,j-1] + S0[t-1,i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | i-1==0 && t==0 && j-2>=0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + S0[0,i,j-1] + A[i,j] + A[i,j+1] + A[i+1,j-1]
			+ A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-1==0 && i-1==0 && t-1>=0 && n-4>=0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + A[i,j-1] + S0[t-1,i,j] +
			S0[t-1,i,j+1] + A[i+1,j-1] + S0[t-1,i+1,j] + S0[t-1,i+1,j+1]) / 9.0 ;
			{ | j-1==0 && i-1==0 && n-3==0 && t-1>=0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + A[i,j-1] + S0[t-1,i,j] + A[i,j+1]
			+ A[i+1,j-1] + A[i+1,j] + A[i+1,j+1]) / 9.0 ;
			{ | j-1==0 && i-1==0 && t==0 } : (A[i-1,j-1] + A[i-1,j] + A[i-1,j+1] + A[i,j-1] + A[i,j] + A[i,j+1] + A[i+1,j-1] +
			A[i+1,j] + A[i+1,j+1]) / 9.0 ;
		esac;
.